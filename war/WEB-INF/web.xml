<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Bookstuf</servlet-name>
		<servlet-class>com.bookstuf.web.BookstufServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Bookstuf</servlet-name>
		<url-pattern>/bookstuf</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>gitkit</servlet-name>
		<servlet-class>com.google.gitkit.samples.WidgetServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>gitkit</servlet-name>
		<url-pattern>/gitkit</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.google.gitkit.samples.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
